generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_PRISMA_URL_URL_NON_POOLING") // uses a direct connection
}

model geo_styles {
  id          Int      @unique @default(autoincrement())
  name        String   @db.VarChar(255)
  description String?
  code        String
  image       String   @db.VarChar(1024)
  created_at  DateTime @default(now()) @db.Timestamp(6)
}
